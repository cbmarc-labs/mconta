<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:mconta.web.client.ui"
	xmlns:f="urn:import:mconta.web.client.view">
	<ui:with field="i18n" type="mconta.web.client.i18n.AppConstants" />
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	
	<g:HTMLPanel>
		
		<ui:text from="{i18n.form_label_use_name}" />
		<g:TextBox ui:field="use_name"/>
		<ui:text from="{i18n.form_label_use_password}" />
		<g:TextBox ui:field="use_password" />
		<ui:text from="{i18n.form_label_use_roles}" />
		<c:ObjectListBox ui:field="use_roles"/>
		
		<g:Button ui:field="submitButton" text="{i18n.common_form_button_submit}"/>
		
		<c:AppCellTable ui:field="appCellTable"/>
		
	</g:HTMLPanel>
	
</ui:UiBinder> 