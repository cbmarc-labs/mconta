<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:mconta.web.client.ui"
	xmlns:f="urn:import:mconta.web.client.view">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	
	<g:HTMLPanel>
	
		UserName :
		<g:TextBox ui:field="use_name"/>
		Password :
		<g:TextBox ui:field="use_password"/>
		Role :
		<c:ObjectListBox ui:field="use_roles"/>
		
		<g:Button ui:field="submitButton" text="Submit"/>
		<g:Button ui:field="deleteButton" text="Delete Selected Items"/>
		
		<c:AppCellTable ui:field="appCellTable"/>
		
	</g:HTMLPanel>
	
</ui:UiBinder> 