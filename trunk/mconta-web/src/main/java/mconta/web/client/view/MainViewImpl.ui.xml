<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:mconta.web.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	
	<g:HTMLPanel>
		<g:TextBox ui:field="titleField"></g:TextBox>
		<g:Button ui:field="submitButton" text="Submit"></g:Button>
		<g:Button ui:field="deleteButton" text="Delete Selected Items"></g:Button>
		<c:AppCellTable ui:field="appCellTable"></c:AppCellTable>
		
		<g:TextBox ui:field="usernameField"></g:TextBox>
		<g:Button ui:field="userSubmitButton" text="Submit"></g:Button>
		<g:Button ui:field="deleteUserButton" text="Delete Selected Items"></g:Button>
		<c:AppCellTable ui:field="appCellTableUser"></c:AppCellTable>
		
		<g:HTMLPanel ui:field="listGrid" />
	</g:HTMLPanel>
	
</ui:UiBinder> 