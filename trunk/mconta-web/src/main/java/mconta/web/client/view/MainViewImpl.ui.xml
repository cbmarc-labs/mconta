<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:mconta.web.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	
	<g:HTMLPanel>
		<g:TextBox ui:field="titleField"></g:TextBox>
		<g:Button ui:field="acceptButton" text="Accept"></g:Button>
		<c:AppDataGrid ui:field="appDataGrid"></c:AppDataGrid>
		<g:HTMLPanel ui:field="listGrid" />
	</g:HTMLPanel>
	
</ui:UiBinder> 