<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.panel {
			margin:25px;
			padding: 25px;
			border:1px solid #aaa;

		}
	</ui:style>
	
	<g:HTMLPanel>
	
		<g:Label>USER PANEL</g:Label>
		<g:HTMLPanel ui:field="userpanel" styleName="{style.panel}"></g:HTMLPanel>
		<g:Label>ROLE PANEL</g:Label>
		<g:HTMLPanel ui:field="rolepanel" styleName="{style.panel}"></g:HTMLPanel>
		
	</g:HTMLPanel>
	
</ui:UiBinder> 