<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:mconta.web.client.ui"
	xmlns:f="urn:import:mconta.web.client.view">
	<ui:with field="i18n" type="mconta.web.client.i18n.AppConstants" />
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	
	<g:HTMLPanel>
	
		<ui:text from="{i18n.form_label_rol_name}" />
		<g:TextBox ui:field="rol_name"></g:TextBox>
		
		<g:Button ui:field="submitButton" text="{i18n.common_form_button_submit}"/>
		<g:Button ui:field="deleteButton" text="{i18n.common_form_button_delete_all}"/>
		
		<c:AppCellTable ui:field="appCellTable"></c:AppCellTable>
		
	</g:HTMLPanel>
	
</ui:UiBinder> 