<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:c="urn:import:mconta.web.client.mobile.ui">
	
	<c:JQMPage ui:field="page">
	    
	    <div data-role="header"><h1>Header Edit</h1></div>
		<div data-role="content">
		    <ul data-role="listview" data-inset="true">
		        <li data-role="fieldcontainer">
		        	<fieldset class="ui-grid-b">
						<div class="ui-block-a"><c:JQMButton ui:field="button_minus" dataicon="minus" dataiconpos="top"></c:JQMButton></div>
						<div class="ui-block-b"><h1 style="text-align:center;font-size:2em;" ui:field="quantity">1</h1></div>
						<div class="ui-block-c"><c:JQMButton ui:field="button_plus" dataicon="plus" dataiconpos="top"></c:JQMButton></div>
					</fieldset>
				</li>
				
		        <li data-role="fieldcontainer">
		        	<fieldset class="ui-grid-a">
						<div class="ui-block-a"><img src="img/album-p.jpg" style="max-height:80px;max-width:80px;"></img></div>
						<div class="ui-block-b"><h3>Broken Bells</h3><p>Broken Bells</p></div>	   
					</fieldset>
				</li>

				<li data-role="fieldcontainer">
		        	<fieldset class="ui-grid-a">
						<div class="ui-block-a"><a href="#" data-role="button" data-theme="c" data-rel="back">Cancelar</a></div>
						<div class="ui-block-b"><a href="#" data-role="button" data-theme="b">Aceptar</a></div>	   
					</fieldset>
				</li>
			</ul>
		</div>
		<div data-role="footer"><h1>Footer</h1></div>
	    
    </c:JQMPage>
    
</ui:UiBinder> 